{
  "name": "@jds/npm_rollup",
  "description": "Example of bundling npm-package via rollup",
  "author": "Denis Krasnovskiy",
  "license": "MIT",
  "version": "1.0.0",
  "repository": "https://github.com/jquery-dlya-slabih/npm_rollup",
  "homepage": "https://github.com/jquery-dlya-slabih/npm_rollup",
  "module": "dist/index.es.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": "*.css",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "18 || >=20",
    "npm": ">=10"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "lodash.uniqueid": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.0.2",
    "@eslint/js": "9.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@rollup/plugin-image": "3.0.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/lodash.uniqueid": "4.0.9",
    "@types/react": "18.2.65",
    "@types/react-dom": "18.2.21",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "autoprefixer": "10.4.18",
    "css-loader": "6.10.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "8.1.1",
    "postcss-url": "10.1.3",
    "prettier": "3.2.5",
    "rollup": "4.13.0",
    "rollup-plugin-node-externals": "7.1.2",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-size": "0.3.1",
    "sass": "1.71.1",
    "sass-loader": "14.1.1",
    "style-loader": "3.3.4",
    "stylelint": "16.2.1",
    "stylelint-config-sass-guidelines": "11.1.0",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "tsc-alias": "1.8.8",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.4.5",
    "typescript-plugin-css-modules": "5.1.0",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "scripts": {
    "dev": "webpack serve --node-env=dev",
    "build": "rm -rf dist && rollup --config --failAfterWarnings && tsc-alias",
    "lint": "run-p lint:*",
    "lint:style": "stylelint \"{src,dev}/**/*.scss\"",
    "lint:es": "eslint --ext .ts,.tsx src/ dev/",
    "prettier:write": "npx prettier . --write",
    "prettier:check": "prettier --check .",
    "test": "jest --config=jest.config.json --silent",
    "test:coverage": "jest --config=jest.config.json --silent --collectCoverage",
    "test:watch": "jest --config=jest.config.json --watch",
    "prepare": "git config --local core.hooksPath .githooks || echo 'Во время установки конфигурации git-хуков произошла ошибка'"
  }
}
